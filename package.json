{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "NestJS boilerplate with Fastify, Sequelize, JWT, and Vitest",
  "author": "Ygor Carvalho",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "biome format --write .",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome check --write .",
    "lint:ci": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "db:migrate": "node scripts/db-migrate.cjs",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npx sequelize-cli db:drop && node scripts/db-migrate.cjs",
    "db:status": "npx sequelize-cli db:migrate:status"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-fastify": "^11.0.1",
    "@nestjs/sequelize": "^11.0.1",
    "@nestjs/swagger": "^11.0.0",
    "bcryptjs": "^2.4.3",
    "nestjs-pino": "^4.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "pino": "^9.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "pino-pretty": "^11.3.0",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^10.13.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "@fastify/static": "^8.0.0"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@nestjs/core",
      "@scarf/scarf",
      "cpu-features",
      "esbuild",
      "protobufjs",
      "ssh2"
    ]
  }
}
